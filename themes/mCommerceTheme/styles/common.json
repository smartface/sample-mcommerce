{
	".grow": {
		"flexProps": {
			"flexGrow": 1
		},
		"height": null
	},
	".noPadding": {
		"paddingLeft": null,
		"paddingRight": null,
		"paddingTop": null,
		"paddingBottom": null
	},
	".noMargin": {
		"marginTop": null,
		"marginBottom": null
	},
	".sticky": {
		"flexProps": {
			"positionType": "ABSOLUTE"
		},
		"bottom": 0,
		"left": 0,
		"right": 0,
		"height": 100
	},
	".padding": {
		"paddingLeft": 25,
		"paddingRight": 25,
		"paddingTop": 15,
		"paddingBottom": 15
	},
	".paddingVertical": {
		"paddingTop": 15,
		"paddingBottom": 15
	},
	".paddingHorizontal": {
		"paddingLeft": 15,
		"paddingRight": 15
	},
	".rowFlex": {
		"flexProps": {
			"flexDirection": "ROW"
		}
	},
	".seperator": {
		"height": 1,
		"left": 15,
		"right": 0,
		"bottom": 0,
		"marginTop": null,
		"marginBottom": null,
		"flexProps": {
			"positionType": "ABSOLUTE"
		}
	},
	".notVisible": {
		"visible": false
	},
	".radius": {
		"borderRadius": 15
	},
	".borderColor": {
		"borderWidth": 1,
		"borderColor": "${grayBorder}"
	}
}
