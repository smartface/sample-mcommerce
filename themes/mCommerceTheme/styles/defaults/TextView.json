{
  ".sf-textView": {
    "masksToBounds": true,
    "width": null,
    "height": 131,
    "maxLines": 1,
    "ellipsizeMode": "END",
    "textAlignment": "MIDLEFT",
    "backgroundColor": "${transparent}",
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    }
  }
}
