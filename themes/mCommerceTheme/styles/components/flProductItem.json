{
	".flProductItem": {
		"paddingTop": 10,
		"marginRight": 15,
		"&-flProductItemWrapper": {
			"paddingLeft": 10,
			"paddingRight": 10,
			"borderRadius": 25,
			"borderColor": "#E2E2E2",
			"borderWidth": 1,
			"backgroundColor": "${transparent}",
			"flexProps": {
				"flexDirection": "COLUMN",
				"justifyContent": "CENTER",
				"alignItems": "CENTER",
				"flexGrow": 1
			},
			"&-productImage": {
				"height": 62,
				"width": 100
			},
			"&-descriptionWrapper": {
				"height": 60,
				"paddingTop": 5,
				"paddingBottom": 5,
				"paddingLeft": 5,
				"paddingRight": 5,
				"flexProps": {
					"alignSelf": "STRETCH",
					"flexDirection": "COLUMN",
					"justifyContent": "CENTER",
					"alignItems": "FLEX_START"
				},
				"&-lblProductTitle": {
					"height": 20,
					"width": 130,
					"textColor": "${textDark}",
					"font": {
						"size": 16,
						"family": "Nunito",
						"style": "Bold"
					}
				},
				"&-lblProductDimension": {
					"height": 20,
					"width": 130,
					"textColor": "${textLight}",
					"font": {
						"size": 14,
						"family": "Nunito",
						"style": "Regular"
					}
				}
			},
			"&-priceButtonWrapper": {
				"height": 60,
				"paddingTop": 5,
				"paddingBottom": 5,
				"paddingLeft": 5,
				"paddingRight": 5,
				"flexProps": {
					"alignSelf": "STRETCH",
					"flexDirection": "ROW",
					"justifyContent": "SPACE_BETWEEN",
					"alignItems": "STRETCH"
				},
				"&-lblProductPrice": {
					"height": null,
					"width": 20,
					"textColor": "${textDark}",
					"font": {
						"size": 18,
						"family": "Nunito",
						"style": "Bold"
					},
					"flexProps": {
						"flexGrow": 1
					}
				}
			},
			"&-flTagWrapper": {
				"backgroundColor": "rgba( 136, 21, 236, 1 )",
				"testId": "_9ziXcn4H",
				"height": 30,
				"top": 0,
				"width": 100,
				"right": 5,
				"borderRadius": 10,
				"flexProps": {
					"flexGrow": 1,
					"positionType": "ABSOLUTE"
				},
				"&-lblTag": {
					"ellipsizeMode": "END",
					"flexProps": {
						"alignSelf": "AUTO",
						"flexGrow": 1
					},
					"font": {
						"size": 10,
						"bold": true,
						"italic": false,
						"family": "Nunito",
						"style": "Medium"
					},
					"marginBottom": null,
					"marginTop": null,
					"textAlignment": "MIDCENTER",
					"textColor": "rgba( 255, 255, 255, 1 )",
					"paddingRight": 10,
					"paddingLeft": 10
				}
			}
		},
		"height": 250,
		"width": null,
		"flexProps": {
			"flexGrow": 1
		}
	}
}