{
    ".gviCategoryItem": {
        "width": 175,
        "height": 190,
        "&-flCategoryItemWrapper": {
            "marginTop": 15,
            "marginRight":7, 
            "marginLeft":7, 
            "paddingTop": 10,
            "paddingBottom": 5,
            "paddingRight": 25,
            "paddingLeft": 25,
            "borderRadius": 18,
            "flexProps": {
                "flexDirection": "COLUMN",
                "justifyContent": "CENTER",
                "flexGrow": 1
            },
            "&-imgCategoryItem": {
                "flexProps": {
                    "flexGrow": 0.7
                }
            },
            "&-lblCategoryItemTitle": {
                "maxHeight": 50,
                "textColor": "${textDark}",
                "flexProps": {
                    "flexGrow": 1
                },
                "font": {
                    "size": 16,
                    "family": "Nunito",
                    "style": "Bold"

                }
            }
        }
    }

}