{
	".flRow2LineButton": {
		"height": 120,
		"&-imgLeft": {
			"width": 80,
			"height": 80,
			"marginRight": 20,
			"flexProps": {
				"alignSelf": "CENTER"
			},
			"borderRadius": 17,
			"imageFillType": "ASPECTFIT"
		},
		"flexProps": {
			"flexDirection": "ROW",
			"flexGrow": 1
		},
		"&-flButtons": {
			"&-flOtherActions": {
				"flexProps": {
					"flexDirection": "ROW"
				},
				"&-btnBottomLeft": {
					"marginRight": 10
				},
				"&-btnBottomRight": {
					"marginLeft": 10
				},
				"marginTop": 15,
				"height": 25,
				"&-verticalSeparator": {
					"backgroundColor": "${textDark}",
					"height": 20,
					"width": 1,
					"marginLeft": 8,
					"marginRight": 8,
					"marginTop": 3
				},
				"&-lblLeft": {
					"textAlignment": "MIDRIGHT"
				},
				"&-lblRight": {
					"textAlignment": "MIDLEFT"
				},
				"marginRight": 10
			},
			"&-secondary": {
				"backgroundColor": "${backgroundSecondary}",
				"textColor": "${textDark}",
				"borderColor": "${textDark}",
				"borderWidth": 2,
				"height": 60
			},
			"&-btnMain": {
				"height": 40,
				"textColor": "${white}",
				"marginRight": 10
			},
			"flexProps": {
				"justifyContent": "CENTER"
			}
		},
		"width": null
	}
}