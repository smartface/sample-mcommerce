{
  ".flRow2LineButton": {
    "height": 120,
    "&-imgLeft": {
      "width": 70,
      "height": 70,
      "marginRight": 15,
      "tintColor": "${textDark}",
      "flexProps": {
        "alignSelf": "CENTER"
      },
      "borderRadius": 35
    },
    "flexProps": {
      "flexDirection": "ROW"
    },
    "&-flButtons": {
      "&-flOtherActions": {
        "flexProps": {
          "flexDirection": "ROW"
        },
        "&-btnBottomLeft": {
          "marginRight": 10
        },
        "&-btnBottomRight": {
          "marginLeft": 10
        },
        "marginTop": 10,
        "height": 25,
        "&-verticalSeparator": {
          "backgroundColor": "${textDark}",
          "height": 25,
          "width": 2
        }
      },
      "&-secondary": {
        "backgroundColor": "${backgroundSecondary}",
        "textColor": "${textDark}",
        "borderColor": "${textDark}",
        "borderWidth": 2,
        "height": 60
      },
      "&-btnMain": {
        "height": 50
      }
    }
  }
}
