{
  ".flProductDetailTitleSection": {
    "flexProps": {
      "alignContent": "CENTER"
    },
    "&-flWrapper": {
      "flexProps": {
        "flexDirection": "ROW"
      },
      "&-flTitleAndDesc": {
        "flexProps": {
          "flexDirection": "COLUMN"
        },
        "testId": "tZKkNycUh",
        "&-lblProductTitle": {
          "textColor": "${textDark}",
          "font": {
            "size": 24,
            "bold": true,
            "italic": false,
            "family": "Nunito",
            "style": "Bold"
          },
          "maxLines": 2,
          "height": 30
        },
        "&-lblProductMeas": {
          "textColor": "${textLight}",
          "font": {
            "size": 16,
            "bold": true,
            "italic": false,
            "family": "Nunito",
            "style": "SemiBold"
          },
          "height": 30,
          "testId": "WhIxHioCx"
        }
      },
      "&-flFavoriteImg": {
        "&-imgFavorite": {
          "flexProps": {
            "flexGrow": 1
          },
          "tintColor": "${favoriIcon}",
          "height": null,
          "testId": "vzb75eWIi"
        }
      }
    },
    "null": "75",
    "height": 75
  }
}
