{
  ".flNoConnection": {
    "flexProps": {
      "alignContent": "CENTER",
      "justifyContent": "CENTER",
      "flexGrow": 1
    },
    "&-btnTryAgain": {
      "marginTop": 15,
      "visible": true,
      "width": null
    },
    "&-imgSuccess": {
      "height": 200,
      "imageFillType": "ASPECTFIT",
      "width": null,
      "usePageVariable": true,
      "image": "images://try_again.png"
    },
    "&-lblTitle": {
      "font": {
        "bold": true,
        "family": "Nunito",
        "style": "Bold",
        "italic": false,
        "size": 25
      },
      "height": 70,
      "marginTop": 25,
      "textAlignment": "MIDCENTER",
      "textColor": "${textDark}",
      "maxLines": 2,
      "text": "label1",
      "usePageVariable": true
    },
    "&-lblDescription": {
      "textAlignment": "MIDCENTER",
      "textColor": "${darkGray}",
      "font": {
        "size": 17,
        "bold": false,
        "italic": false,
        "family": "Nunito",
        "style": "Regular"
      },
      "maxLines": 2,
      "text": "label1",
      "usePageVariable": true
    },
    "&-btnTrackOrder": {
      "marginTop": 100,
      "visible": false,
      "text": "button1",
      "usePageVariable": true
    },
    "&-lblHome": {
      "font": {
        "size": 16,
        "bold": true,
        "italic": false,
        "family": "Nunito",
        "style": "Bold"
      },
      "textAlignment": "MIDCENTER",
      "textColor": "${textDark}",
      "text": "label1",
      "usePageVariable": true
    },
    "paddingLeft": 50,
    "paddingRight": 50
  }
}