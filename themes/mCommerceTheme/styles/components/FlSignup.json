{
    ".flSignup": {
        "flexProps": {
            "flexGrow": 1
        },
        "&-imgLogo": {
            "flexProps": {
                "alignSelf": "CENTER",
                "flexGrow": 1.5
            },
            "height": null,
            "width": 70,
            "imageFillType": "ASPECTFIT",
            "image": "logo.png"
        },
        "&-lblTitle": {
            "marginBottom": null,
            "marginTop": null,
            "marginLeft": 25,
            "flexProps": {
                "flexGrow": 0.5
            }
        },
        "&-lblText": {
            "flexProps": {
                "flexGrow": 0.5
            },
            "marginBottom": 25,
            "marginLeft": 25,
            "marginTop": null,
            "maxHeight": 35
        },
        "&-flForm": {
            "flexProps": {
                "flexGrow": 3
            },
            "height": null,
            "paddingLeft": 25,
            "paddingRight": 25,
            "paddingBottom": 10
        },
        "&-mtbDefault": {
            "borderColor": "${textLight}",
            "flex": {
                "positionType": 0
            },
            "flexProps": {
                "positionType": "RELATIVE"
            },
            "left": 0,
            "top": 0,
            "usePageVariable": true
        },
        "&-tvTermsAndPrivacy": {
            "backgroundColor": "${backgroundMain}",
            "height": 70,
            "maxLines": 2
        }
    }
}